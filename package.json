{
    "name": "@fleetbase/console",
    "version": "0.0.1",
    "private": true,
    "description": "Fleetbase Console",
    "repository": "",
    "license": "MIT",
    "author": "",
    "directories": {
        "doc": "doc",
        "test": "tests"
    },
    "scripts": {
        "reload": "yarn upgrade @fleetbase/ember-core @fleetbase/ember-ui @fleetbase/flb-fleetops-extension && yarn start",
        "build": "ember build --environment=production",
        "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
        "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
        "lint:hbs": "ember-template-lint .",
        "lint:hbs:fix": "ember-template-lint . --fix",
        "lint:js": "eslint . --cache",
        "lint:js:fix": "eslint . --fix",
        "start": "ember serve",
        "test": "npm-run-all lint test:*",
        "test:ember": "ember test",
        "relink": "yarn link @fleetbase/ember-ui && yarn link @fleetbase/ember-core && yarn link @fleetbase/flb-fleetops-extension",
        "reupdate-full": "yarn upgrade @fleetbase/ember-ui @fleetbase/ember-core @fleetbase/flb-fleetops-extension",
        "reset-core": "rm -rf ../ember-core/node_modules && yarn --cwd ../ember-core install",
        "reset": "rm -rf node_modules yarn.link && yarn && yarn relink && yarn start",
        "devsetup": "yarn && cd ../ember-core && yarn && yarn link && cd ../ember-ui && yarn && yarn link && cd ../flb-fleetops-extension && yarn && yarn link && cd ../console && yarn relink"
    },
    "devDependencies": {
        "@ember/optional-features": "^2.0.0",
        "@ember/test-helpers": "^2.8.1",
        "@fortawesome/ember-fontawesome": "^0.4.1",
        "@fortawesome/free-solid-svg-icons": "^6.2.0",
        "@glimmer/component": "^1.1.2",
        "@glimmer/tracking": "^1.1.2",
        "@tailwindcss/forms": "^0.5.3",
        "autoprefixer": "^10.4.8",
        "babel-eslint": "^10.1.0",
        "broccoli-asset-rev": "^3.0.0",
        "date-fns": "^2.29.2",
        "ember-auto-import": "^2.4.2",
        "ember-cli": "~4.6.0",
        "ember-cli-app-version": "^5.0.0",
        "ember-cli-babel": "^7.26.11",
        "ember-cli-dependency-checker": "^3.3.1",
        "ember-cli-dotenv": "^3.1.0",
        "ember-cli-es6-transform": "^1.0.0",
        "ember-cli-htmlbars": "^6.1.0",
        "ember-cli-inject-live-reload": "^2.1.0",
        "ember-cli-postcss": "^8.2.0",
        "ember-cli-sri": "^2.1.1",
        "ember-cli-string-helpers": "^6.1.0",
        "ember-cli-terser": "^4.0.2",
        "ember-collapsible-panel": "^5.0.0",
        "ember-data": "~4.4.0",
        "ember-engines": "^0.8.23",
        "ember-fetch": "^8.1.1",
        "ember-file-upload": "^7.1.0",
        "ember-leaflet": "^5.0.1",
        "ember-load-initializers": "^2.1.2",
        "ember-page-title": "^7.0.0",
        "ember-qunit": "^5.1.5",
        "ember-resolver": "^8.0.3",
        "ember-responsive": "^5.0.0",
        "ember-source": "~4.6.0",
        "ember-template-lint": "^4.10.1",
        "ember-wormhole": "^0.6.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-ember": "^11.0.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-qunit": "^7.3.1",
        "fleetbase-extensions-indexer": "^0.0.1",
        "inter-ui": "^3.19.3",
        "leaflet": "^1.9.2",
        "loader.js": "^4.7.0",
        "normalize.css": "^8.0.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.17",
        "postcss-conditionals-renewed": "^1.0.0",
        "postcss-each": "^1.1.0",
        "postcss-mixins": "^9.0.4",
        "postcss-preset-env": "^7.8.2",
        "postcss-simple-vars": "^7.0.0",
        "prettier": "^2.7.1",
        "qunit": "^2.19.1",
        "qunit-dom": "^2.0.0",
        "socketcluster": "^17.2.0",
        "tailwindcss": "^3.1.8",
        "webpack": "^5.74.0"
    },
    "engines": {
        "node": "14.* || >= 16"
    },
    "ember": {
        "edition": "octane"
    },
    "dependencies": {
        "@ember/legacy-built-in-components": "^0.4.1",
        "@fleetbase/ember-core": "../ember-core",
        "@fleetbase/ember-ui": "../ember-ui",
        "@fleetbase/flb-fleetops-extension": "../flb-fleetops-extension",
        "@fleetbase/flb-storefront-extension": "../flb-storefront-extension",
        "@fleetbase/leaflet-routing-machine": "^3.2.15",
        "ember-composable-helpers": "^5.0.0",
        "ember-power-select": "^6.0.1",
        "postcss-at-rules-variables": "^0.3.0",
        "postcss-custom-properties": "^12.1.9",
        "postcss-nth-list": "^1.0.2"
    },
    "resolutions": {
        "ember-cli-notifications/postcss-preset-env": "^7.8.2",
        "ember-engines/broccoli-babel-transpiler": "^v8.0.0-beta.1",
        "ember-engines/broccoli-babel-transpiler/broccoli-persistent-filter": "^3.1.3"
    }
}
